{
  "name": "barrio_sass",
  "version": "8.4.1",
  "description": "Barrio SASS implementation",
  "main": "index.js",
  "scripts": {
    "dev": "sass ./scss/style.scss ./css/style.css",
    "production": "sass --style=compressed ./scss/style.scss ./css/style.min.css",
    "watch": "sass --watch ./scss/style.scss ./css/style.css"
  },
  "keywords": [
    "SASS",
    "Bootstrap",
    "Drupal",
    "Barrio"
  ],
  "author": "@hatuhay",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "sass": "^1.55.0"
  }
}
